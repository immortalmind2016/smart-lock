{  
   "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,         
      "strict": true,
      "skipLibCheck": true ,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "noImplicitAny": false,
      "allowJs": true
    },

    "exclude": ["dist","test","jest.config.ts","tsconfig.json","ormconfig.ts"],
    "include": ["src/**/*"],


}
{
  "name": "Stella",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mohamed Salah <immortal.mind2016@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch:dev": "nodemon -e .ts -x yarn start:dev",
    "start:dev": "ts-node ./src/index.ts",
    "start": "node ./dist/index.js",
    "build:ts": "tsc",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/configs/data-source.ts",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "generate-token:dev": "cross-env CLI=true ts-node ./src/modules/token/utils/generate-token",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "migration:run": "ts-node ./src/scripts/run-migrations.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/configs/data-source.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "apollo-datasource": "^3.3.2",
    "apollo-server": "^3.10.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.1",
    "bull": "^4.8.4",
    "bullmq": "^1.86.7",
    "class-validator": "^0.13.2",
    "cuid": "^2.1.8",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.3",
    "graphql": "^15.3.0",
    "node-schedule": "^2.1.0",
    "pg": "^8.4.0",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.9",
    "type-graphql": "^1.1.1",
    "typeorm": "0.3.6",
    "typeorm-seeding": "^1.6.1",
    "worker-farm": "^1.7.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/jest": "^28.1.5",
    "@types/node-cron": "^3.0.2",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-jest": "^28.1.3",
    "better-sqlite3": "^7.6.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.6",
    "ts-node": "10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.5.2"
  },
  "lint-staged": {
    "*.ts": "eslint --ext .ts --fix"
  }
}
